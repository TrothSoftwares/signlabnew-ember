
<div class="ten wide column ">


<div class="ui black segment" style="background:rgba(256,256,256,0.4)">
  <div class="ui basic segment">
      <h2 class="ui header"> Current Enquiries </h2>
      <table class="ui very basic table">
      <thead>
        <tr>
          <th>Enquiry.No</th>
          <th>Customer</th>
          <th>Agent </th>
          <th></th>
        </tr>
      </thead>
      <tbody>





        {{#each enquiries as |enquiry|}}
      <tr>
          <td>{{enquiry.id}}</td>
          <td>{{enquiry.project.customer.name}}</td>
          <td>{{enquiry.project.agent.name}}</td>

          {{#link-to 'dashboard.projects.project.home' enquiry.project.id}}
          <td><button class="mini blue ui button">Open</button></td>
          {{/link-to}}


</tr>
{{/each}}
      </tbody>
    </table>
    </div>
</div>

</div>

{{outlet}}
